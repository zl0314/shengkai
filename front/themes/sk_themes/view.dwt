                $("#fn" + obj).val(parseInt($("#fn" + obj).val()) - 1);
                $("#tn" + obj).val(parseInt($("#tn" + obj).val()) - 1);
            }
        }

    };

    //去程获取目的城市
    var get_to_region_plane = function (from_city, to_city){
        var from_city_id = Number($("#" + from_city).val());
        if (isNaN(from_city_id) || from_city_id == 0) {
            $("#from_to_city").select2('val', 0);
            $("#to_from_city").select2('val', 0);
            $("#to_to_city").select2('val', 0);
            $("input[name=from_form_time]").val('');
            $("input[name=to_form_time]").val('');
            $("#from_air").select2('val', 0);
            $("#to_air").select2('val', 0);
            return false;
        }

        $.ajax({
            url: 'view.php?act=ajax_get_air_line_list_from_city',
            type: 'POST',
            dataType: 'json',
            data: {from_city: from_city_id, type: 1},
            success: function (data) {
                $("#" + to_city).html("<option value='0'>请选择</option>").select2('val', 0);
                if (from_city == "from_from_city" || from_city == "from_to_city") {
                    $("#from_to_city").select2('val', 0);
                    $("#to_from_city").select2('val', 0);
                    $("#to_to_city").select2('val', 0);
                    $("input[name=from_form_time]").val('');
                    $("input[name=to_form_time]").val('');
                    $("#from_air").select2('val', 0);
                    $("#to_air").select2('val', 0);
                } else if (from_city == "to_from_city" || from_city == "to_to_city") {
                    $("#to_air").select2('val', 0);
                    $("#to_form_time").val('');
                }
                $.each(data, function (index, item){
                    $("#" + to_city).append("<option value=" + item.region_id + ">" + item.region_name + "</option>");
                });
            }
        });
    };

    //返程出发城市获取目标城市列表
    var get_to_region_plane_2 = function (from_city, to_city){
        var from_city_id = Number($("#" + from_city).val());
        if (isNaN(from_city_id) || from_city_id == 0) {
            return false;
        }

        $.ajax({
            url: 'view.php?act=ajax_get_air_line_list_from_city',
            type: 'POST',
            dataType: 'json',
            data: {from_city: from_city_id, type: 2},
            success: function (data) {
                $("#" + to_city).html("<option value='0'>请选择</option>").select2('val', 0);
                if (from_city == "from_from_city" || from_city == "from_to_city") {
                    $("#from_air").select2('val', 0);
                } else if (from_city == "to_from_city" || from_city == "to_to_city") {
                    $("#to_air").select2('val', 0);
                }
                $.each(data, function (index, item){
                    $("#" + to_city).append("<option value=" + item.region_id + ">" + item.region_name + "</option>");
                });
            }
        });
    };

    /*
     * 获取航班机票
     */
    var get_plane = function (from, to, change) {
        var from_obj = document.getElementById(from);
        var to_obj = document.getElementById(to);
        var from_index = from_obj.selectedIndex;
        var to_index = to_obj.selectedIndex;
        var from_city = from_obj.options[from_index].value;
        var to_city = to_obj.options[to_index].value;
        $("#" + change).empty();
        if ((from_city == 0) || (to_city == 0)) {
            return false;
        }
        $.ajax({
            url: 'view.php?act=ajax_get_air_line_list',
            type: 'POST',
            dataType: 'json',
            data: {from_city: from_city, to_city: to_city},
            success: function (data) {
                $("#" + change).append("<option value='0'>请选择</option>");
                $("#" + change).select2('val', 0);
                for (var air in data) {
                    $("#" + change).append("<option value=" + data[air].id + " data-description='" + encodeURIComponent(data[air].description)+ "'>" + data[air].title + " 时间：" + data[air].fly_time + "</option>");
                }

                //设置返程
                var val = $("#from_from_city").val();
                var text = $("#from_from_city").find(":selected").text();
                $("#to_to_city").html("<option value='0'>请选择</option><option value='" + val + "'>" + text + "</option>")
                    .select2('val', val);

                //设置返程
                var val2 = $("#from_to_city").val();
                var text2 = $("#from_to_city").find(":selected").text();
                $("#to_from_city").html("<option value='0'>请选择</option><option value='" + val2 + "'>" + text2 + "</option>")
                    .select2('val', val2);

                $("#to_air").html("<option value='0'>请选择</option>");
                $("#to_air").select2('val', 0);
                for (var air in data) {
                    $("#to_air").append("<option value=" + data[air].id + ">" + data[air].title + " 时间：" + data[air].return_fly_time + "</option>");
                }

                $("input[name=from_form_time]").val('');
                $("input[name=to_form_time]").val('');
            }
        });
    };

    var set_plane = function (select_id, div_id) {
        var air_line_id = $("#" + select_id).val();
        if (air_line_id == 0 || air_line_id == "") {
            $("input[name=from_form_time]").val('');
            $("input[name=to_form_time]").val('');
        }
        var options = {
            locale:  'zh-cn',
            format: 'YYYY-MM-DD',
            viewMode: 'days',
            useCurrent: false,
            defaultDate: false,
//                keepOpen: true,
//                showClose: true,
            allowInputToggle: true,
            collapse: true,
            widgetPositioning: {
                horizontal: 'left',
                vertical: 'auto'
            }
        };
        //机票

        if (typeof datepicker_from == "object" && typeof datepicker_from.data("DateTimePicker") == "object") {
            datepicker_from.data("DateTimePicker").destroy();
            $("input[name=from_form_time]").val('');
        }

        if (typeof datepicker_return == "object" && typeof datepicker_return.data("DateTimePicker") == "object") {
            datepicker_return.data("DateTimePicker").destroy();
            $("input[name=to_form_time]").val('');
        }

        $("#to_air").select2('val', air_line_id);
        var description = $("#from_air").find("option:selected").attr("data-description");
        $("#air_line_description").html(description ? decodeURIComponent(description) : '');

        from_allow_date_list = ['1970-01-01'];
        return_allow_date_list = ['1970-01-01'];
        $.ajax({url: 'view.php?act=ajax_air_line_num', type: 'POST', dataType: 'json', data: {air: air_line_id}, success: function (data) {
            if (typeof data == 'object') {
                for (var i = 0; i < data.length; i ++) {
                    if (data[i].num < 1) {
                        continue;
                    }
                    if (data[i].type == 1) {
                        from_allow_date_list.push(data[i].date);
                    } else if (data[i].type == 2) {
                        return_allow_date_list.push(data[i].date);
                    }
                }

                options.defaultDate = false;
                if (from_allow_date_list.length > 1) {
                    options.defaultDate = new Date(from_allow_date_list[1]);
                }
                datepicker_from = $("input[name=from_form_time]").datetimepicker(options);
                datepicker_from.data("DateTimePicker").enabledDates(from_allow_date_list);

                options.defaultDate = false;
                if (return_allow_date_list.length > 1) {
                    options.defaultDate = new Date(return_allow_date_list[1]);
                }
                datepicker_return = $("input[name=to_form_time]").datetimepicker(options);
                datepicker_return.data("DateTimePicker").enabledDates(return_allow_date_list);
            }
        }});
    };

    var check_zuoxi = function (obj, num) {
        if (obj.checked) {
            $("#fd" + num).attr("class", "shuliang");
            $("#td" + num).attr("class", "shuliang");
            obj.parentNode.nextSibling.nextSibling.setAttribute("class", "shuliang");
        } else {
            $("#fd" + num).attr("class", "shuliang hidden");
            $("#td" + num).attr("class", "shuliang hidden");
            $("#tn" + num).val(0);
            $("#fn" + num).val(0);
            obj.parentNode.nextSibling.nextSibling.setAttribute("class", "shuliang hidden");
        }
    };

    //酒店
    var from_allow_date_list_hotel = [], return_allow_date_list_hotel = [];
    var check_room = function (obj, room_id, room_type, min_num) {
        if (obj.checked) {
            obj.parentNode.parentNode.nextSibling.nextSibling.setAttribute("class", "");
        } else {
            obj.parentNode.parentNode.nextSibling.nextSibling.setAttribute("class", "hidden");
            return false;
        }

        if (min_num < 1) {
            alert("最小预定天数不能小于1");
            return false;
        }
        var options = {
            locale:  'zh-cn',
            format: 'YYYY-MM-DD',
            viewMode: 'days',
            useCurrent: false,
            defaultDate: false,
//                keepOpen: true,
            showClose: true,
            allowInputToggle: true,
            collapse: true,
            minDate: new Date(),
            widgetPositioning: {
                horizontal: 'left',
                vertical: 'auto'
            }
        };
        from_allow_date_list_hotel = ['1971-01-01'];
        return_allow_date_list_hotel = ['1971-01-01'];
        var tmp = ['1971-01-01'];

        var date = new Date();
        var year = date.getFullYear();
        var month = date.getMonth() + 1;
        var day = date.getDate();
        var currDateTs = new Date(year + "-" + (month > 9 ? "" : "0") + month + "-" + (day > 9 ? "" : "0") + day).valueOf() + 24 * 60 * 60 * 1000 - 1000;;
        $.ajax({url:"view.php?act=ajax_room_num", dataType:"json", data:{room_id:room_id, room_type:room_type}, success: function (result){
            for (var i = 0; i < result.length; i ++) {
                if (result[i].num < 1) {
                    //console.log("库存日期" + result[i].date + "小于当前日期" + year + "-" + month + "-" + day);
                    continue;
                }
                if ((new Date(result[i].date)).valueOf() < currDateTs) {
                    //console.log("库存日期" + result[i].date + "小于当前日期" + year + "-" + month + "-" + day);
                    continue;
                }
                from_allow_date_list_hotel.push(result[i].date);
                //return_allow_date_list_hotel.push(result[i].date);
                tmp.push(result[i].date);
            }

            options.defaultDate = false;
            if (from_allow_date_list_hotel.length > 1) {
                options.defaultDate = new Date(from_allow_date_list_hotel[1]);
            }

            if (!options.defaultDate) {
                alert("该酒店房间类型库存不足");
                return false;
            }

            //检查是否有最小天数限制
            if (min_num > 0) {
                var ty = options.defaultDate.getFullYear(), tm = options.defaultDate.getMonth() + 1, td = options.defaultDate.getDate();
                tm = tm > 9 ? tm : "0" + tm;
                td = td > 9 ? td : "0" + td;
                var d1 = addDate(ty + "-" + tm + "-" + td, Number(min_num));
                var year2 = d1.getFullYear();
                var month2 = d1.getMonth() + 1;
                var day2 = d1.getDate();
                return_allow_date_list_hotel.push(year2 + "-" + (month2 > 9 ? month2 : "0" + month2) + "-" + (day2 > 9 ? day2 : "0" + day2));
            } else {
                return_allow_date_list_hotel = tmp;
            }

            if (return_allow_date_list_hotel.length == 0) {
                return_allow_date_list_hotel.push("1971-01-01");
            }
            //酒店
            $("#start_date_" + room_id + "_" + room_type).datetimepicker(options).data("DateTimePicker").enabledDates(from_allow_date_list_hotel);

            options.defaultDate = false;
            if (return_allow_date_list_hotel.length > 1) {
                options.defaultDate = new Date(return_allow_date_list_hotel[1]);
            }

            if (!options.defaultDate) {
                alert("该酒店房间类型库存不足");
                return false;
            }

            $("#start_date_" + room_id + "_" + room_type).on("dp.change", function (e){
                if (min_num < 1) {
                    return true;
                }

                var tdate = (new Date(e.date));
                var ty = tdate.getFullYear(), tm = tdate.getMonth() + 1, td = tdate.getDate();
                tm = tm > 9 ? tm : "0" + tm;
                td = td > 9 ? td : "0" + td;

                var d1 = addDate(ty + "-" + tm + "-" + td, Number(min_num));

                $("#end_date_" + room_id + "_" + room_type).val(d1.getFullYear() + "-" + (d1.getMonth() + 1 > 9 ? "" : "0") + (d1.getMonth() + 1)  + "-" + (d1.getDate() > 9 ? "" : "0") + d1.getDate());
            });
            if (min_num > 0) {
                $("#end_date_" + room_id + "_" + room_type).val(return_allow_date_list_hotel[1]).attr("readonly", "readonly");
                $("#end_date_" + room_id + "_" + room_type).parent().parent().next().html("单次必须预定" + min_num + "天");
            } else {
                $("#end_date_" + room_id + "_" + room_type).datetimepicker(options).data("DateTimePicker").enabledDates(return_allow_date_list_hotel);
            }
        }});

    };

    function addDate(dd,dadd){
        var a = new Date(dd);
        a = a.valueOf();
        a = a + dadd * 24 * 60 * 60 * 1000;
        a = new Date(a);
        return a;
    }

    function cencel_hotel(hotel_id) {
        document.getElementById(hotel_id).style.display = "none";
        document.getElementById(hotel_id).checked = false;
    }

    var set_number = function (obj, flag) {
        if (flag == "+") {
            $("#" + obj).val(parseInt($("#" + obj)[0].value) + 1);
        } else {
            if (parseInt($("#" + obj)[0].value) >= 1) {
                $("#" + obj).val(parseInt($("#" + obj)[0].value) - 1);
            }
        }
    };

    var set_space_number = function (obj, flag) {
        if (flag == "+") {
            $("#" + obj).val(parseInt($("#" + obj)[0].value) + 1);
            $("#" + obj).val(parseInt($("#" + obj)[0].value) + 1);
        } else {
            if (parseInt($("#" + obj)[0].value) >= 1) {
                $("#" + obj).val(parseInt($("#" + obj)[0].value) - 1);
            }
        }
    };
    var submit_form = function () {
        var i = 0;
        $('[name="from_number[]"]').each(function () {
            if ($(this).val() != 0) {
                i++;
            }
        });
        var res = "";
        if ($('#from_from_city').val() == 0) {
            res = "false";
        } else if ($('#from_to_city').val() == 0) {
            res = "false";
        } else if ($('[name="form_form_time"]').val() == '') {
            res = "false";
        } else if ($('[name="form_to_time"]').val() == '') {
            res = "false";
        } else if ($('#from_air').val() == 0) {
            res = "false";
        } else if (i == 0) {
            res = "false";
        } else if ($('#to_from_city').val() == 0) {
            res = "false";
        } else if ($('#to_to_city').val() == 0) {
            res = "false";
        } else if ($('[name="to_form_time"]').val() == '') {
            res = "false";
        } else if ($('[name="to_to_time"]').val() == '') {
            res = "false";
        } else if ($('#to_air').val() == 0) {
            res = "false";
        } else {
            document.getElementById("air_form").submit();
            return true;
        }

        if (res == "false") {
            $('#cuowu').css("display", "block");
        }
    };
    function cencel_hotel(hotel_id) {
        document.getElementById(hotel_id).style.display = "none";
        document.getElementById(hotel_id).checked = false;
    }
    function check_cart(user_id, game_id, cart_num) {
        if ($(".ticket_list").length == 0) {
            alert("没有球票不能结算，请先购买球票~");
            return false;

        }
        if (user_id == 0) {
            show_login();
        } else {
            var somedialog = document.getElementById("somedialog");
            var dlg = new DialogFx(somedialog);
            $("div.xieyi").scroll(function () {
                //按钮enable
                var _this = $(this);
                var viewH = _this.height();
                var contentH = _this.get(0).scrollHeight;
                var scrollTop = _this.scrollTop();
                var rate = scrollTop / (contentH - viewH);


                if (rate < 0.9) {
                    $("#agree_button").attr("disabled", true);
                    $("#agree_button").removeClass("anniu");
                    $("#agree_button").addClass("anniu_disable");
                    $("#disagree_button").attr("disabled", true);
                    $("#disagree_button").removeClass("anniu");
                    $("#disagree_button").addClass("anniu_disable");
                } else {
                    $("#agree_button").attr("disabled", false);
                    $("#agree_button").removeClass("anniu_disable");
                    $("#agree_button").addClass("anniu");
                    $("#disagree_button").attr("disabled", false);
                    $("#disagree_button").removeClass("anniu_disable");
                    $("#disagree_button").addClass("anniu");
                }
            });

            dlg.toggle();

        }
    }

    $("#air_form").click(function (e) {
        var login_top = $("#display_plane").offset().top;
        var login_left = $("#display_plane").offset().left;
        var login_bottom = $("#display_plane").offset().top + $("#display_plane").height();
        var login_right = $("#display_plane").offset().left + $("#display_plane").width();
        var is_close = 1;
        if (e.pageX > login_left && e.pageX < login_right && e.pageY > login_top && e.pageY < login_bottom) {
            is_close = 0;
        }
        if (is_close) {
            $("#ticket_info").css("display", "none");
        }
    });

