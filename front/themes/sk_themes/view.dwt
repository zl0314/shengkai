        var login_left = $("#display_ambitu").offset().left;
        var login_bottom = $("#display_ambitu").offset().top + $("#display_ambitu").height();
        var login_right = $("#display_ambitu").offset().left + $("#display_ambitu").width();
        var is_close = 1;
        if (e.pageX > login_left && e.pageX < login_right && e.pageY > login_top && e.pageY < login_bottom) {
            is_close = 0;
        }
        if (is_close) {
            $("#ambitu_goods").css("display", "none");
        }
