            <script>
            var time = 0;
            var timeIntervel = 2000;
            function myInterval(){
                var myInterval = setInterval(function(){
                    var order_sn = $("#WIDout_trade_no_99").val();
                    $.ajax({
                        type:'get',
                        url:'user.php?act=select_order_status&order_sn='+order_sn,
                        dataType:'text',
                        success:function(data){
                            //console.log(data);
                            time += timeIntervel / 1000;
                            if(data == 'true'){
                                clearInterval(myInterval);
                                location.href="bearer.php?act=index&out_trade_no="+order_sn;
                            }else if(time > 60){
                                clearInterval(myInterval);
                                time = 0;
                            }
                        }
                    });
                },timeIntervel);
            }
